<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">


<menu id="root-menu" label="Openbox 3">

  <item label="Sublime">
    <action name="Execute">
       <command>subl</command>
    </action>
  </item>

  <item label="Firefox">
    <action name="Execute">
       <command>firefox</command>
    </action>
  </item>

  <item label="Thunar">
    <action name="Execute">
       <command>Thunar</command>
    </action>
  </item>

  <item label="URxvt">
    <action name="Execute">
      <command>urxvt</command>
    </action>
  </item>

  <item label="Ncmpcpp">
    <action name="Execute">
       <command>urxvt -e ncmpcpp</command>
    </action>
  </item>

  <item label="Gimp">
    <action name="Execute">
      <command>gimp</command>
    </action>
  </item>

  <item label="Log Out">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
</menu>

</openbox_menu>
